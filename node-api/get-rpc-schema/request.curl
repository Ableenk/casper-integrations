# -----------------------------------------------------------
# Retrieve JSON-RPC schema for discovery purposes.
# -----------------------------------------------------------
curl --location -g --request POST 'http://3.136.227.9:7777/rpc' \
--data-raw '{
    "id": 1,
    "jsonrpc": "2.0",
    "method": "rpc.discover"
}'

# -----------------------------------------------------------
# Retrieve JSON-RPC schema & display supported endpoints.
# -----------------------------------------------------------
curl --location -g --request POST 'http://3.136.227.9:7777/rpc' \
--data-raw '{
    "id": 1,
    "jsonrpc": "2.0",
    "method": "rpc.discover"
}' | jq '.result.schema.methods[].name'
